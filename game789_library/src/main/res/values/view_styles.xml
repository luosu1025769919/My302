<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="edit">

        <item name="android:layout_width">215dp</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:background">@drawable/luosu_shape_edit</item>
        <item name="android:digits">@string/luosu_edit_digits</item>
        <item name="android:inputType">textPersonName</item>


    </style>

    <style name="activity_style" parent="Theme.AppCompat.Light.NoActionBar">
